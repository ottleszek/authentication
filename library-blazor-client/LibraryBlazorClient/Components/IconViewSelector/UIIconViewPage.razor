@namespace LibraryBlazorClient.Components

@if (Parent.ActualPage == this)
{
    @ChildContent
}

@code {
    [CascadingParameter] public UIIconViewSelector? Parent { get; set; }
    [Parameter] public UIIconPageType? PageType { get; set; }
    [Parameter] public RenderFragment? ChildContent { get; set; }

    protected override void OnInitialized()
    {
        if (Parent is not null)
        {
            Parent.AddPage(this);
        }
    }
}
